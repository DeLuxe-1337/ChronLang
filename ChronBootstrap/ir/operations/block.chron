IR.Write.Block :: (context) {
    foreach index, value in Table.Iter(context) {
        if TypeOf(index) == "number" {
            IR.Write(value)
        }
    }
    
    return nil
}

IR.CreateBlock :: () {
    return <"type"="Block">
}

IR.Block.Append :: (block, statement) {
    index = Table.SizeOf(block) - 1
    block[index] = statement
}