include core.all

["return"]
Test :: {}

Main :: {
    defer PrintLn("End of program")
    PrintLn("Hello, world")
    PrintLn(true)
    PrintLn(false)
    PrintLn(nil)

    PrintLn(2 + 3)
    PrintLn(5 - 2)
    PrintLn(2 * 4)
    PrintLn(10 / 2)
    PrintLn(3 + 4 * 2) 
    PrintLn((3 + 4) * 2)
    PrintLn(6 / (2 + 1))
    PrintLn(500 * 2)
